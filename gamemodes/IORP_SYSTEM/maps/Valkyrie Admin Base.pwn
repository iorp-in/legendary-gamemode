public spawnVehiculeWithPlayerInto(playerid, vehicle_num, Float:x, Float:y, Float:z, Float:angle, Float:variationZ) {
    new vehicleid = CreateVehicle(vehicle_num, x, y, z + variationZ, angle, -1, -1, 99999999);
    PutPlayerInVehicleEx(playerid, vehicleid, 0);
    LinkVehicleToInterior(vehicleid, GetPlayerInterior(playerid));
    SetCameraBehindPlayer(playerid);
    TogglePlayerControllable(playerid, 1);
}

public moveObject(objectid, Float:posx, Float:posy, Float:posz, Float:speed) {
    MoveDynamicObject(objectid, posx, posy, posz, speed);
}

