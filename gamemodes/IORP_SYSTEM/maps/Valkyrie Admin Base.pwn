Float:GetDistance(playerid, Float:x2, Float:y2, Float:z2) {
    new Float:x1, Float:y1, Float:z1;
    GetPlayerPos(playerid, x1, y1, z1);
    return Float:floatsqroot(floatpower(floatabs(floatsub(x2, x1)), 2) + floatpower(floatabs(floatsub(y2, y1)), 2) + floatpower(floatabs(floatsub(z2, z1)), 2));
}

hook OnPlayerRequestShop(playerid, shopid) {
    if (shopid == 24) ShowMenuForPlayer(actionmenu, playerid);
    return 1;
}

public OnPlayerSelectedMenuRow(playerid, row) {
    new Menu:PlayerMenu = GetPlayerMenu(playerid);

    if (PlayerMenu == actionmenu) {
        switch (row) {
            case 0 :  //Secure Missile Area
            {
                //close missile ZONE entrance GATES
                MoveDynamicObject(area_gate1, 984.69293212891, 3713.9858398438, 6.3888874053955, 1.5);
                MoveDynamicObject(area_gate2, 976.75402832031, 3714.3483886719, 6.3952875137329, 1.5);
                v_is_zone_secure = 1;
                SendClientMessageEx(playerid, 0xAA3333AA, "Main gates are now closed!");
                ShowMenuForPlayer(actionmenu, playerid);
            }
            case 1 :  //Open Missile Vault
            {
                //open missile HOLE door
                MoveDynamicObject(missile_door1, 941.35894775391, 3907.2556152344, 16.438453674316, 1.0);
                MoveDynamicObject(missile_door2, 954.86999511719, 3897.8276367188, 16.44441986084, 1.0);
                MoveDynamicObject(missile_door3, 964.48608398438, 3910.7839355469, 16.438020706177, 1.0);
                MoveDynamicObject(missile_door4, 951.00958251953, 3920.5446777344, 16.450819015503, 1.0);

                //close the fences
                MoveDynamicObject(missile_fence_east, 962.9677734375, 3911.5002441406, 10.10812664032, 0.25);
                MoveDynamicObject(missile_fence_south_east, 962.64447021484, 3903.4624023438, 10.133727073669, 0.25);
                MoveDynamicObject(missile_fence_south, 952.52972412109, 3899.0085449219, 10.087343215942, 0.25);
                MoveDynamicObject(missile_fence_south_west, 942.81585693359, 3906.7077636719, 10.144944190979, 0.25);
                MoveDynamicObject(missile_fence_north_west, 943.10327148438, 3914.7778320313, 10.127326965332, 0.25);
                MoveDynamicObject(missile_fence_north, 953.3134765625, 3919.3310546875, 10.101726531982, 0.25);

                //close missile house doors
                MoveDynamicObject(underground_door1, 956.947265625, 3862.7607421875, 18.05940246582, 1.5);
                MoveDynamicObject(underground_door2, 944.5244140625, 3863.259765625, 18.065801620483, 1.5);
                MoveDynamicObject(house_door, 952.89978027344, 3860.9560546875, 20.822633743286, 1.5);
                v_is_vault_open = 1;
                SendClientMessageEx(playerid, 0xAA3333AA, "Missile Silo is now opening... Blast doors closed!");
                ShowMenuForPlayer(actionmenu, playerid);
            }
            case 2 :  //Launch Missile
            {
                if (v_is_zone_secure == 0) {
                    SendClientMessageEx(playerid, 0xAA3333AA, "You need to secure the zone first!");
                    return 1;
                }
                if (v_is_vault_open == 0) {
                    SendClientMessageEx(playerid, 0xAA3333AA, "You need to open the vault to launch a missile!");
                    return 1;
                }

                if ((v_can_missile_launch == 1) && (v_is_vault_open == 1) && (v_is_zone_secure == 1)) {

                    valbase_countdown = 10;

                    TimerCountdown = SetPreciseTimer("startCountdown", 1000, true);

                    SetPreciseTimer("launchMissile", 11000, false);

                    SendClientMessageToAll(0xAA3333AA, "Missile launch in progress.....");
                } else {
                    SendClientMessageEx(playerid, 0xAA3333AA, "You have to wait 30 seconds to launch a new missile!");
                }
                ShowMenuForPlayer(actionmenu, playerid);

            }
            case 3 :  //Close Missile Vault
            {
                //close the fences
                MoveDynamicObject(missile_fence_east, 961.259765625, 3911.58203125, 10.10812664032, 1.5);
                MoveDynamicObject(missile_fence_south_east, 960.9345703125, 3903.541015625, 10.133727073669, 1.5);
                MoveDynamicObject(missile_fence_south, 952.63671875, 3901.7099609375, 10.087343215942, 1.5);
                MoveDynamicObject(missile_fence_south_west, 944.5498046875, 3906.73828125, 10.144944190979, 1.5);
                MoveDynamicObject(missile_fence_north_west, 944.8525390625, 3914.681640625, 10.127326965332, 1.5);
                MoveDynamicObject(missile_fence_north, 953.240234375, 3917.427734375, 10.101726531982, 1.5);

                //close the missile doors
                MoveDynamicObject(missile_door1, 950.26983642578, 3906.9921875, 16.350164413452, 1.5);
                MoveDynamicObject(missile_door2, 956.90960693359, 3906.8095703125, 16.337366104126, 1.5);
                MoveDynamicObject(missile_door3, 957.04638671875, 3912.2607421875, 16.330966949463, 1.5);
                MoveDynamicObject(missile_door4, 950.40887451172, 3912.4611816406, 16.343765258789, 1.5);

                //open the house doors
                MoveDynamicObject(underground_door1, 956.947265625, 3862.7607421875, 15.240211486816, 1.5);
                MoveDynamicObject(underground_door2, 944.5244140625, 3863.259765625, 15.239867210388, 1.5);
                MoveDynamicObject(house_door, 952.80236816406, 3859.0634765625, 20.822633743286, 1.5);
                SendClientMessageToAll(0xAA3333AA, "Missile Silo is now closing...Blast doors are open!");
                ShowMenuForPlayer(actionmenu, playerid);
            }
            case 4 :  //Open Missile Area
            {
                //reopen the area
                MoveDynamicObject(area_gate1, 984.69293212891, 3713.9858398438, 1.6382882595062, 1.5);
                MoveDynamicObject(area_gate2, 976.75402832031, 3714.3483886719, 1.6388874053955, 1.5);
                SendClientMessageToAll(0xAA3333AA, "Main gates are now open!");
                ShowMenuForPlayer(actionmenu, playerid);
            }
            case 5 :  {
                HideMenuForPlayer(actionmenu, playerid);
            }
        }
        TogglePlayerControllable(playerid, 1);
    }

    return 1;
}

public OnPlayerExitedMenu(playerid) {
