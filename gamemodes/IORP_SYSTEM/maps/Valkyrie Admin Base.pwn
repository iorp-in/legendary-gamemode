public checkPlayerInZone(playerid) {
    //door1
    if ((GetDistance(playerid, 1016.7558, 3288.7788, 8.9837) <= 25.0) || (GetDistance(playerid, 1008.8359, 3289.0454, 8.9837) <= 25.0)) {
        MoveDynamicObject(door1, 1021.7802, 3288.9296, 8.9737, 1.5);
        MoveDynamicObject(door2, 1003.9121, 3289.1611, 8.9737, 1.5);

        SetTimerEx("moveObject", 6000, false, "iffff", door1, 1016.7558, 3288.7788, 8.9837, 1.5);
        SetTimerEx("moveObject", 6000, false, "iffff", door2, 1008.8359, 3289.0454, 8.9837, 1.5);
