    if (v_can_missile_launch == 0) {
        new Float:posx, Float:posy, Float:posz;
        GetDynamicObjectPos(missile_part12, posx, posy, posz);
        CreateExplosion(posx, posy, posz + 3, 7, 20.0);
    } else {
        DeletePreciseTimer(explosions);
    }
    return 1;
}

