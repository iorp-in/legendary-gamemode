    //lift
    if (GetPlayerDistanceFromPoint(playerid, 836.7231, 3381.2673, 10.6982) <= 40.0) MoveDynamicObject(carrier_lift1, 836.72314453125, 3381.2673339844, 17.648227882385, 1.5);
