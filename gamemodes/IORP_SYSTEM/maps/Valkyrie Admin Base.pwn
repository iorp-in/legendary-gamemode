public startCountdown() {
    if (valbase_countdown != 10) TextDrawDestroy(valbase_count);
    new string[25];
    if (valbase_countdown == 0) {
        format(string, sizeof string, "~r~%s", "Blastoff!");
        valbase_count = TextDrawCreate(498.0, 110.0, string);
    } else {
        format(string, sizeof string, "~r~%i", valbase_countdown);
        valbase_count = TextDrawCreate(498.0, 110.0, string);
    }
    if (valbase_countdown >= 0) {
        TextDrawAlignment(valbase_count, 2);
        TextDrawUseBox(valbase_count, 1);
        TextDrawFont(valbase_count, 3);
        TextDrawTextSize(valbase_count, 110, 30);
        TextDrawBoxColor(valbase_count, 0x0000AA66);
        TextDrawShowForAll(valbase_count);
    }
    if (valbase_countdown == -1) {
        TextDrawDestroy(valbase_count);
        DeletePreciseTimer(TimerCountdown);
        TextDrawHideForAll(valbase_count);
    }
    valbase_countdown = valbase_countdown - 1;
    return 1;
}