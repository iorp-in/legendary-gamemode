#define MAX_STORAGE_TYPE_FARMING_ITEMS 698

stock DynamicShopBusinessItem:GetShopStock(shopid, shopItemId) {
    for (new i; i < MAX_STORAGE_TYPE_FARMING_ITEMS; i++) {
        if (i == shopItemId) return DynamicShopBusiness:DatabaseGetItem(shopid, sprintf("shopItemId_%d", shopItemId), 0);
    }
    return 0;
}

stock DynamicShopBusinessItem:UpdateShopStock(shopid, shopItemId, amount) {
    for (new i; i < MAX_STORAGE_TYPE_FARMING_ITEMS; i++) {
        if (i == shopItemId) return DynamicShopBusiness:DatabaseUpdateItem(shopid, sprintf("shopItemId_%d", shopItemId), amount);
    }
    return 0;
}

stock DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId) {
    for (new i; i < MAX_STORAGE_TYPE_FARMING_ITEMS; i++) {
        if (i == shopItemId) return DynamicShopBusiness:DatabaseGetItem(shopid, sprintf("shopItemId_%d_price", shopItemId), DynamicShopBusinessItem:GetItemMaxPrice(shopItemId));
    }
    return 0;
}

stock DynamicShopBusinessItem:UpdateShopPrice(shopid, shopItemId, amount) {
    for (new i; i < MAX_STORAGE_TYPE_FARMING_ITEMS; i++) {
        if (i == shopItemId) return DynamicShopBusiness:DatabaseUpdateItem(shopid, sprintf("shopItemId_%d_price", shopItemId), amount);
    }
    return 0;
}

hook OnDyShopRequestUpdate(shopid, bool:databaseupdate) {
    if (DynamicShopBusiness:GetType(shopid) == Shop_Type_Farming_Resources ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Farming_Seeds ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_General_Items ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Material_Items) {
        new string[2000];
        strcat(string, sprintf("\n"));
        new type = DynamicShopBusiness:GetType(shopid);
        new total = DynamicShopBusinessItem:ArrayOfIdsSize[type];
        for (new i; i < total; i++) {
            new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
            strcat(string, sprintf("{DC143C}%s:{FFFFFF}$%s/%s [%d/%d %s]\n",
                DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId)),
