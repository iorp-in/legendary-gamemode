        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_HeavyMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_TwoWheelerMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_HelicopterMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_PlaneMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_BoatMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Clothe_Accessories ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Garage
    ) {
        DynamicShopBusiness:Update3dText(shopid, sprintf("\n{0080FF}open shop menu to purchase goods or to view list available stock\n"), databaseupdate);
        return ~1;
    }
    return 1;
}

stock DynamicShopBusinessItem:ShowInHandMenu(playerid, shopid, page = 0, menu_type = DIALOG_STYLE_PREVIEW_MODEL) {
    new type = DynamicShopBusiness:GetType(shopid);
    new string[2000];
    if (menu_type == DIALOG_STYLE_TABLIST_HEADERS) strcat(string, "ID\tName\tCurrent Price\tCurrent Stock\n");
    new total = DynamicShopBusinessItem:ArrayOfIdsSize[type];
    new perPage = 20;
    if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) perPage = 34;
    if (total > perPage) {
        new paged = page * perPage;
        new skip = page * perPage;
        new count = 0;
        for (new i; i < total; i++) {
            new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
            if (count >= perPage) break;
            if (skip > 0) {
                skip--;
                continue;
            }
            if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) strcat(string, sprintf("%d\t%s~n~~g~~h~$%s~n~~y~Stock: %d\n", DynamicShopBusinessItem:GetItemModelID(shopItemId), DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId)), DynamicShopBusinessItem:GetShopStock(shopid, shopItemId)));
            else strcat(string, sprintf("{FFFF00}%d\t{0080FF}%s\t{00FF00}$%s\t{0080FF}%d\n", shopItemId, DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId)), DynamicShopBusinessItem:GetShopStock(shopid, shopItemId)));
            count++;
        }
        if ((total - (paged + perPage)) > 0) {
            if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) { strcat(string, "19134\t~w~Next Page\n"); } else strcat(string, "Next Page\n");
        }
        if (page > 0) {
            if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) { strcat(string, "19133\t~w~Back Page\n"); } else strcat(string, "Back Page\n");
        }
    } else {
        for (new i; i < total; i++) {
            new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
            if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) strcat(string, sprintf("%d\t%s~n~~g~~h~$%s~n~~y~Stock: %d\n", DynamicShopBusinessItem:GetItemModelID(shopItemId), DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId)), DynamicShopBusinessItem:GetShopStock(shopid, shopItemId)));
            else strcat(string, sprintf("{FFFF00}%d\t{0080FF}%s\t{00FF00}$%s\t{0080FF}%d\n", shopItemId, DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId)), DynamicShopBusinessItem:GetShopStock(shopid, shopItemId)));
        }
    }
    FlexPlayerDialog(playerid, "ShopBussPurchase", menu_type, sprintf("%s", DynamicShopBusiness:GetName(shopid)), string, "Select", "Close", shopid, sprintf("%d %d", page, menu_type));
    return 1;
}

FlexDialog:ShopBussPurchase(playerid, response, listitem, const inputtext[], extraid, const payload[]) {
    if (!response) return 1;
    new shopid = extraid, page, menu_type;
    sscanf(payload, "d d", page, menu_type);
    if (!strcmp(inputtext, "Next Page") || strval(inputtext) == 19134) return DynamicShopBusinessItem:ShowInHandMenu(playerid, shopid, page + 1, menu_type);
    if (!strcmp(inputtext, "Back Page") || strval(inputtext) == 19133) return DynamicShopBusinessItem:ShowInHandMenu(playerid, shopid, page - 1, menu_type);
    new shopItemId = strval(inputtext);
    if (menu_type == DIALOG_STYLE_PREVIEW_MODEL) {
        new fid = page * 34 + listitem;
        new type = DynamicShopBusiness:GetType(shopid);
        shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][fid];
    }
    CallRemoteFunction("OnPurchaseFromShop", "ddd", playerid, shopid, shopItemId);
    return 1;
}

