        new skip = page * 20;
        new count = 0;
        for (new i; i < total; i++) {
            new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
            if (count >= 20) break;
            if (skip > 0) {
                skip--;
                continue;
            }
            strcat(string, sprintf("%d\t%s\t$%s\n", shopItemId, DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId))));
            count++;
        }
        if ((total - (paged + 20)) > 0) strcat(string, "Next Page\n");
        if (page > 0) strcat(string, "Back Page\n");
    } else {
        for (new i; i < total; i++) {
            new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
            strcat(string, sprintf("%d\t%s\t$%s\n", shopItemId, DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(DynamicShopBusinessItem:GetShopPrice(shopid, shopItemId))));
        }
    }
    FlexPlayerDialog(playerid, "ShopBussUpdatePrice", DIALOG_STYLE_TABLIST_HEADERS, "Update Pricing", string, "Select", "Close", shopid, sprintf("%d", page));
    return 1;
}

FlexDialog:ShopBussUpdatePrice(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return DynamicShopBusiness:OwnerMenu(playerid, shopid);
    if (!strcmp(inputtext, "Next Page")) return DynamicShopBusinessItem:UpdatePriceOptions(playerid, shopid, strval(payload) + 1);
    if (!strcmp(inputtext, "Back Page")) return DynamicShopBusinessItem:UpdatePriceOptions(playerid, shopid, strval(payload) - 1);
    DynamicShopBusinessItem:UpdatePriceInput(playerid, shopid, strval(inputtext));
    return 1;
}

stock DynamicShopBusinessItem:UpdatePriceInput(playerid, shopid, shopItemId) {
    return FlexPlayerDialog(playerid, "ShopBussUpdatePriceInput", DIALOG_STYLE_INPUT, "Update Pricing", sprintf(
        sprintf(
            "Enter price between $%s to $%s",
            FormatCurrency(DynamicShopBusinessItem:GetItemMinPrice(shopItemId)),
            FormatCurrency(DynamicShopBusinessItem:GetItemMaxPrice(shopItemId))
        )
    ), "Update", "Close", shopid, sprintf("%d", shopItemId));
}

FlexDialog:ShopBussUpdatePriceInput(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return DynamicShopBusinessItem:UpdatePriceOptions(playerid, shopid);
    new newprice, shopItemId = strval(payload);
    if (
        sscanf(inputtext, "d", newprice) && newprice < 1 ||
        newprice < DynamicShopBusinessItem:GetItemMinPrice(shopItemId) ||
        newprice > DynamicShopBusinessItem:GetItemMaxPrice(shopItemId)
    ) return DynamicShopBusinessItem:UpdatePriceInput(playerid, shopid, shopItemId);
    DynamicShopBusinessItem:UpdateShopPrice(shopid, shopItemId, newprice);
    SendClientMessage(playerid, -1, sprintf("{0080ff}[Alexa]:{FFFFFF} updated %s price to $%s", DynamicShopBusinessItem:GetItemName(shopItemId), FormatCurrency(newprice)));
    DynamicShopBusinessItem:UpdatePriceOptions(playerid, shopid);
    DynamicShopBusiness:RequestUpdate(shopid);
    return 1;
}

stock DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid, page = 0) {
    new string[2000], type = DynamicShopBusiness:GetType(shopid);
    strcat(string, "ID\tName\tIn Storage\tIn Trailer\n");
    new total = DynamicShopBusinessItem:ArrayOfIdsSize[type];
    new paged = page * 20;
    new skip = page * 20;
    new count = 0;
    for (new i; i < total; i++) {
        new shopItemId = DynamicShopBusinessItem:ArrayOfIds[type][i];
        if (count >= 20) break;
        if (skip > 0) {
            skip--;
            continue;
        }
        new inShop = DynamicShopBusinessItem:GetShopStock(shopid, shopItemId);
        new inTrailer = TrailerStorage:GetResourceByShopId(trailerid, shopItemId);
        if (inShop > 0 || inTrailer > 0) {
            strcat(string, sprintf(
                "%d\t%s\t%d %s\t%d %s\n",
                shopItemId,
                DynamicShopBusinessItem:GetItemName(shopItemId),
                inShop,
                DynamicShopBusinessItem:GetItemUnit(shopItemId),
                inTrailer,
                DynamicShopBusinessItem:GetItemUnit(shopItemId)
            ));
            count++;
        }
    }
    if (count == 0) strcat(string, "No stocks\n");
    if ((total - (paged + 20)) > 0) strcat(string, "Next Page\n");
    if (page > 0) strcat(string, "Back Page\n");

    FlexPlayerDialog(
        playerid, "ShopBussTruckerMenu", DIALOG_STYLE_TABLIST_HEADERS, "{0080FF}Shop:{FFFF00} Trucker Menu",
        string, "Select", "Close", shopid, sprintf("%d %d", trailerid, page)
    );
    return 1;
}

FlexDialog:ShopBussTruckerMenu(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return 1;
    new trailerid, resourceid, page;
    sscanf(payload, "dd", trailerid, page);
    if (IsStringSame(inputtext, "No stocks")) return DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid, page);
    if (IsStringSame(inputtext, "Next Page")) return DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid, page + 1);
    if (IsStringSame(inputtext, "Back Page")) return DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid, page - 1);
    sscanf(inputtext, "d", resourceid);

    DynamicShopBusiness:TruckerChoice(playerid, shopid, trailerid, resourceid);
    return 1;
}

stock DynamicShopBusiness:TruckerChoice(playerid, shopid, trailerid, resourceid) {
    new string[512];
    if (DynamicShopBusiness:IsAllowedUnLoading(shopid)) strcat(string, "Unload Resource\n");
    if (DynamicShopBusiness:IsAllowedLoading(shopid)) strcat(string, "Load Resource\n");
    return FlexPlayerDialog(
        playerid, "ShopBussTruckerChoice", DIALOG_STYLE_LIST, "{0080FF}Shop:{FFFF00} Trucker Menu", string,
        "Select", "Close", shopid, sprintf("%d %d", trailerid, resourceid)
    );
}

FlexDialog:ShopBussTruckerChoice(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return 1;
    new trailerid, resourceid;
    sscanf(payload, "d d", trailerid, resourceid);
    if (!strcmp(inputtext, "Unload Resource")) return DynamicShopBusiness:TruckerUnloadResourceInput(playerid, shopid, trailerid, resourceid);
    if (!strcmp(inputtext, "Load Resource")) return DynamicShopBusiness:TruckerloadResourceInput(playerid, shopid, trailerid, resourceid);
    return 1;
}

stock DynamicShopBusiness:TruckerUnloadResourceInput(playerid, shopid, trailerid, resourceid) {
    return FlexPlayerDialog(
        playerid, "TruckerUnloadResourceInput", DIALOG_STYLE_INPUT, "{0080FF}Shop:{FFFF00} Trucker Menu",
        sprintf(
            "Shop Storage: %d\nIn Trailer: %d\nEnter amount of resource to unload",
            DynamicShopBusinessItem:GetShopStock(shopid, resourceid),
            TrailerStorage:GetResourceByShopId(trailerid, resourceid)
        ),
        "Unload", "Cancel", shopid, sprintf("%d %d", trailerid, resourceid)
    );
}

FlexDialog:TruckerUnloadResourceInput(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return 1;
    new trailerid, resourceid;
    sscanf(payload, "d d", trailerid, resourceid);
    new amount;
    if (
        sscanf(inputtext, "d", amount) ||
        amount < 1 || amount > 1000 ||
        amount > TrailerStorage:GetResourceByShopId(trailerid, resourceid) ||
        DynamicShopBusinessItem:GetShopStock(shopid, resourceid) + amount > 1000
    ) return DynamicShopBusiness:TruckerUnloadResourceInput(playerid, shopid, trailerid, resourceid);
    TrailerStorage:SetResourceByShopId(trailerid, resourceid, TrailerStorage:GetResourceByShopId(trailerid, resourceid) - amount);
    DynamicShopBusinessItem:UpdateShopStock(shopid, resourceid, DynamicShopBusinessItem:GetShopStock(shopid, resourceid) + amount);
    DynamicShopBusiness:RequestUpdate(shopid);
    SendClientMessage(playerid, -1, sprintf("{0080ff}[Alexa]:{FFFFFF} unloaded %dKG %s in shop storage", amount, DynamicShopBusinessItem:GetItemName(resourceid)));
    DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid);
    return 1;
}

stock DynamicShopBusiness:TruckerloadResourceInput(playerid, shopid, trailerid, resourceid) {
    return FlexPlayerDialog(
        playerid, "TruckerloadResourceInput", DIALOG_STYLE_INPUT, "{0080FF}Shop:{FFFF00} Trucker Menu",
        sprintf(
            "Shop Storage: %d\nIn Trailer: %d\nEnter amount of resource to load",
            DynamicShopBusinessItem:GetShopStock(shopid, resourceid),
            TrailerStorage:GetResourceByShopId(trailerid, resourceid)
        ),
        "Load", "Cancel", shopid, sprintf("%d %d", trailerid, resourceid)
    );
}

FlexDialog:TruckerloadResourceInput(playerid, response, listitem, const inputtext[], shopid, const payload[]) {
    if (!response) return 1;
    new trailerid, resourceid;
    sscanf(payload, "d d", trailerid, resourceid);
    new amount;
    if (
        sscanf(inputtext, "d", amount) ||
        amount < 1 || amount > 1000 ||
        amount > DynamicShopBusinessItem:GetShopStock(shopid, resourceid) ||
        TrailerStorage:GetResourceByShopId(shopid, resourceid) + amount > 1000 ||
        DynamicShopBusinessItem:GetShopPrice(shopid, resourceid) * amount > GetPlayerCash(playerid)
    ) return DynamicShopBusiness:TruckerloadResourceInput(playerid, shopid, trailerid, resourceid);
    GivePlayerCash(playerid, -DynamicShopBusinessItem:GetShopPrice(shopid, resourceid) * amount, sprintf("purchased %dKG %s from %s [%d]", amount, DynamicShopBusinessItem:GetItemName(resourceid), DynamicShopBusiness:GetName(shopid), shopid));
    DynamicShopBusiness:IncreaseBalance(shopid, DynamicShopBusinessItem:GetShopPrice(shopid, resourceid) * amount, sprintf("%s purchased farming item: %dKG of %s", GetPlayerNameEx(playerid), amount, DynamicShopBusinessItem:GetItemName(resourceid)));
    TrailerStorage:SetResourceByShopId(trailerid, resourceid, TrailerStorage:GetResourceByShopId(trailerid, resourceid) + amount);
    DynamicShopBusinessItem:UpdateShopStock(shopid, resourceid, DynamicShopBusinessItem:GetShopStock(shopid, resourceid) - amount);
    DynamicShopBusiness:RequestUpdate(shopid);
    SendClientMessage(playerid, -1, sprintf("{0080ff}[Alexa]:{FFFFFF} loaded %dKG %s in trailer", amount, DynamicShopBusinessItem:GetItemName(resourceid)));
    DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid);
    return 1;
}

