hook OnDyShopRequestTrucker(playerid, trailerid, shopid) {
    if (DynamicShopBusiness:GetType(shopid) == Shop_Type_Farming_Resources ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Farming_Seeds ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_General_Items ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Material_Items ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Electronic ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Clinic ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Firework ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Weapons ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Sex_Toy ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Clothes ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_LightMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_HeavyMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_TwoWheelerMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_HelicopterMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_PlaneMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Dealership_BoatMotor ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Clothe_Accessories ||
        DynamicShopBusiness:GetType(shopid) == Shop_Type_Garage
    ) {
        if (DynamicShopBusiness:IsLocked(shopid)) { SendClientMessage(playerid, 0x0080FF, "[Alexa]:{FFFFFF} Store is closed, please wait for owner to open it."); return ~1; }
        if (!DynamicShopBusiness:IsAllowedLoading(shopid) && !DynamicShopBusiness:IsAllowedUnLoading(shopid)) { SendClientMessage(playerid, 0x0080FF, "[Alexa]:{FFFFFF} Store is open, but trucker actions are allowed."); return ~1; }
        DynamicShopBusinessItem:Trucker(playerid, trailerid, shopid);
        return ~1;
    }
    return 1;
}

