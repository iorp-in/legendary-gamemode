CMD:anim(playerid, const params[]) {
    Anim:SetState(playerid, true);
    if (IsStringContainWords(params, "help, list")) return Anim_Commands(playerid);
    if (IsStringContainWords(params, "surrender, handsup") && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return SetPlayerSpecialAction(playerid, SPECIAL_ACTION_HANDSUP);
    if (IsStringContainWords(params, "sit")) {
        new anim;
        new string[1000];
        format(string, sizeof string, "%s", params);
        strreplace(string, "sit", "", true);
        if (sscanf(string, "d", anim)) return AlexaMsg(playerid, "/anim sit [1-5]");
        switch (anim) {
            case 1:
                ApplyAnimation(playerid, "BEACH", "bather", 4.1, 1, 0, 0, 0, 0, 1);
