cmd:animx(playerid, const params[]) {
    new animId = -1;
    if (sscanf(params, "d", animId) || animId < 0 || animId >= 1776) return ShowAnimMenu(playerid);
    AnimApplyDirect(playerid, animId);
    return 1;
}

stock ShowAnimMenu(playerid) {
    new string[2000];
    for (new i; i < sizeof Anim:Lib; i++) {
        strcat(string, sprintf("%s\n", Anim:Lib[i]));
    }
    return FlexPlayerDialog(playerid, "ShowAnimMenu", DIALOG_STYLE_TABLIST, "Select library", string, "Select", "Close");
}

FlexDialog:ShowAnimMenu(playerid, response, listitem, const inputtext[], extraid, const payload[]) {
    if (!response) return 1;
    ShowAnimSubMenu(playerid, inputtext);
    return 1;
}

stock ShowAnimSubMenu(playerid, const lib[]) {
    new string[2000];
    for (new i; i < 1776; i++) {
        if (IsStringSame(Anim:Sub[i][Anim:Title], lib)) {
            strcat(string, sprintf("%d\t%s\n", i, Anim:Sub[i][Anim:Name]));
        }
    }
    return FlexPlayerDialog(playerid, "ShowAnimSubMenu", DIALOG_STYLE_TABLIST, sprintf("Anim:Lib for %s", lib), string, "Select", "Close");
}

FlexDialog:ShowAnimSubMenu(playerid, response, listitem, const inputtext[], extraid, const lib[]) {
    if (!response) return ShowAnimMenu(playerid);
    new animId = strval(inputtext);
    AnimApplyDirect(playerid, animId);
    return 1;
}

stock AnimApplyDirect(playerid, animId) {
    if (animId < 0 || animId >= 1776) return AlexaMsg(playerid, "invalid animationid");
    return FlexPlayerDialog(
        playerid, "AnimationInput", DIALOG_STYLE_INPUT, "Apply Animation", "[loop] [lockx] [locky] [freeze] [time] [forcesync]", "Apply", "Cancel", animId
    );
}

FlexDialog:AnimationInput(playerid, response, listitem, const inputtext[], animId, const payload[]) {
    if (!response) return ShowAnimSubMenu(playerid, Anim:Sub[animId][Anim:Title]);
    new Float:fDelta = 4.1, loop = 1, lockx = 0, locky = 0, freezex = 0, time = 0, forcesync = 1;
    sscanf(inputtext, "dddddd", loop, lockx, locky, time, freezex, forcesync);
    ApplyAnimation(playerid, Anim:Sub[animId][Anim:Title], Anim:Sub[animId][Anim:Name], Float:fDelta, loop, lockx, locky, freezex, time, forcesync);
    Anim:SetState(playerid, true);
    return 1;
}

